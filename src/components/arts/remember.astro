---
import CardLayout from "./cardLayout.astro";
---

<CardLayout title="remember" hrefname="remember">
  <div class="remember"></div>
</CardLayout>

<style>
  .remember {
    width: 40rem;
    height: 30rem;
    margin: 0 auto;
    position: relative;
  }

  .remember::before {
    width: 40rem;
    height: 30rem;
    content: "";
    position: absolute;
    top: 0;
    left: 2rem;
    background:
    
    /*first pics*/
      linear-gradient(bisque, bisque) 9.45% 40% / 1rem 3rem,
      repeating-linear-gradient(90deg, darkcyan 0 10%, transparent 0 15%) 3.3%
        44% / 6rem 0.5rem,
      repeating-linear-gradient(90deg, darkcyan 0 10%, transparent 0 15%) 3.3%
        59% / 6rem 0.5rem,
      linear-gradient(bisque, bisque) 10% 0% / 0.5rem 12rem,
      linear-gradient(brown, brown) 2% 52% / 7rem 6rem,
      /*second pics*/ linear-gradient(bisque, bisque) 33% 47% / 1rem 3rem,
      linear-gradient(bisque, bisque) 33.2% 0% / 0.5rem 13rem,
      linear-gradient(yellowgreen, bisque) 30.5% 59.5% / 6rem 5rem,
      linear-gradient(white, white) 30% 60% / 7rem 6rem,
      /*third pics*/ linear-gradient(bisque, bisque) 57.4% 42% / 1rem 3rem,
      repeating-linear-gradient(90deg, darkcyan 0 10%, transparent 0 15%) 58.5%
        46% / 6rem 0.5rem,
      repeating-linear-gradient(90deg, darkcyan 0 10%, transparent 0 15%) 58.5%
        62% / 6rem 0.5rem,
      linear-gradient(bisque, bisque) 57.2% 0% / 0.5rem 12rem,
      linear-gradient(brown, brown) 59% 55% / 7rem 6rem,
      /*four pics*/ linear-gradient(bisque, bisque) 82.4% 55% / 1rem 3rem,
      linear-gradient(bisque, bisque) 82% 0% / 0.5rem 15rem,
      linear-gradient(yellowgreen, bisque) 86.8% 69% / 6rem 5rem,
      linear-gradient(white, white) 88% 70% / 7rem 6rem;
    background-repeat: no-repeat;
  }
</style>
