---
interface Props {
  hrefname: string;
  title: string;
}

const { title, hrefname } = Astro.props;
---

<div class="footer"><a href={hrefname}></a></div>

<style>
  .footer {
    background-color: #f4f4f4;
    color: white;
    z-index: 999;
    text-decoration: none;
    width: 100%;
    padding: 0.5rem;
  }
</style>
